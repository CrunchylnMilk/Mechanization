
scoreboard players set $temp_1 mechanization.data 432
execute if entity @s[tag=mech_upgraded] run scoreboard players set $temp_1 mechanization.data 360
execute if entity @s[tag=mech_upgrade_ender] run scoreboard players set $temp_1 mechanization.data 288

execute if data storage du:temp obj.fluid_item.tag.ctc.traits.liquid{id:"molten_iron"} run scoreboard players operation @s mechanization.stored_fluid -= $temp_1 mech_data
execute if score $temp_0 mechanization.data matches ..0 if data storage du:temp obj.fluid_item.tag.ctc.traits.liquid{id:"molten_iron"} run loot replace block ~ ~ ~ container.12 loot mechanization:base/iron_rod
execute if score $temp_0 mechanization.data matches 1.. if data storage du:temp obj.fluid_item.tag.ctc.traits.liquid{id:"molten_iron"} if data storage du:temp obj.output_item.tag.ctc.traits{rod:1b,"metal/iron":1b} run item modify block ~ ~ ~ container.12 mechanization:increment_count
